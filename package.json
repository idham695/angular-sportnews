{
  "name": "sportnews",
  "version": "1.0.0",
  "description": "sportnews website using angular js,  mysql, sequelize, express and node js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "angular",
    "js",
    "mysql",
    "sequelize",
    "express",
    "and",
    "node",
    "js"
  ],
  "author": "Idham Kholid",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.8.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "sequelize": "^6.3.4"
  }
}
